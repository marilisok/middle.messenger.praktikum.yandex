{
  "compilerOptions": {                  
    "target": "ES2017",                          
    "module": "commonjs",                     
    "sourceMap": true,                     
    "strict": true,                           
    "noImplicitAny": true,                 
    "strictNullChecks": true,             
    "noUnusedLocals": true,               
    "noUnusedParameters": true,           
    "esModuleInterop": true,                 
    "skipLibCheck": true,                     
    "forceConsistentCasingInFileNames": true  
  }
}
